{
	"schemaVersion": 1,
	"id": "fabricauth",
	"version": "${version}",
	"name": "FabricAuth",
	"description": "Authing done easy.",
	"authors": [
		"DoctorOne"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/TheDoctorOne/fabricauth"
	},
	"license": "CC0-1.0",
	"icon": "assets/fabricauth/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.mahmutkocas.FabricAuth"
		]
	},
	"mixins": [
		"fabricauth.mixins.json",
		{
			"config": "fabricauth.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.10",
		"minecraft": "~1.20.1",
		"java": ">=21",
